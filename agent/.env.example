# LiveKit Configuration
LIVEKIT_URL=wss://your-livekit-cloud-url
LIVEKIT_API_KEY=your-api-key
LIVEKIT_API_SECRET=your-api-secret

# Language Model
OPENAI_API_KEY=sk-your-openai-key

# Cartesia (STT + TTS)
CARTESIA_API_KEY=your-cartesia-api-key
CARTESIA_STT_MODEL=ink  # Cartesia Ink for STT
CARTESIA_TTS_MODEL=sonic-3  # Cartesia Sonic-3 for TTS
CARTESIA_VOICE_ID=default  # Will be set per call based on mood

# Supermemory
SUPERMEMORY_API_KEY=your-supermemory-api-key
SUPERMEMORY_BASE_URL=https://api.supermemory.ai

# Database (Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Agent Configuration
AGENT_PERSONALITY=supportive
AGENT_NAME=You+ Assistant
